#include <stdio.h>

int sumOfDigits(int m) {
    int sum = 0;

    while (m > 0) {
        sum =sum + m % 10;  
        m = m / 10;     
    }

    return sum;          
}

int main() {
    int n;
    printf("Enter a number: ");
    scanf("%d", &n);

    int result = sumOfDigits(n);
    printf("Sum of digits = %d", result);

    return 0;
}
